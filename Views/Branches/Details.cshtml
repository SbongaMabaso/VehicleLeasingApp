@model VehicleLeasingApp.Models.Branches

@{
	ViewData["Title"] = "Details";
}

<h1>Branch Details</h1>

<div>
	<hr />
	<dl class="row">
		<dt class="col-sm-2">
			@Html.DisplayNameFor(model => model.Name)
		</dt>
		<dd class="col-sm-10">
			@Html.DisplayFor(model => model.Name)
		</dd>
		<dt class="col-sm-2">
			@Html.DisplayNameFor(model => model.Location)
		</dt>
		<dd class="col-sm-10">
			@Html.DisplayFor(model => model.Location)
		</dd>
	</dl>
</div>
<div class="row p-3">
	<div class="form-group">
		<a class="btn btn-primary float-end" asp-action="Edit" asp-route-id="@Model?.BranchId">Edit Branch</a>
		<a class="btn btn-outline-info float-start" asp-action="Index">Cancel</a>
	</div>
</div>
